
<!-- Business Contact List -->

<div class="wrapper justify-content-md-center"><h1>Business contacts list</h1>
  <div class="col">
    <a href="/contacts-list/add" class="btn btn-primary btn-lg">
      <i class="fas fa-plus"></i>Add New Contracts</a>
      <hr>
    <table class="table table-striped table-dark">
        <thead>
          <tr>
            <th scope="col">#</th>
            <th scope="col">Contact name</th>
            <th scope="col">Contact number</th>
            <th scope="col">Email address</th>
          </tr>
        </thead>
        <tbody>
        <!--Repeated-->
        <% for (let i = 0; i < contactList.length; i++) { %>
          <tr>
            <th scope="row"><%= i %></th>
            <td><%= contactList[i].name %></td>
            <td><%= contactList[i].number %></td>
            <td><%= contactList[i].email %></td>
            <td>
             <a href="/contacts-list/update/<%= contactList[i].id %>" class="btn-primary">
             <i class="fas fa-pencil-alt"></i>Update</a>
             </td>
             <td>
             <a href="/contacts-list/delete/<%= contactList[i].id %>" class="btn-danger">
             <i class="fas fa-trash-alt"></i>Delete</a>
             </td>
          </tr>
          <% } %>
        </tbody>
      </table>
</div>
</div>



